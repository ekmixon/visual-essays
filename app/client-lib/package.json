{
  "name": "visual-essays",
  "version": "1.0.0",
  "description": "Visual essay js library",
  "main": "webpack.config.js",
  "scripts": {
    "serve": "vue-cli-service --port 8088 serve main.js",
    "serve-lib": "webpack-dev-server --mode development --hot --port 8088 --output-public-path js --contentBase public --disableHostCheck",
    "build": "webpack --mode production --progress",
    "bundle-report": "webpack-cli --profile --json > compilation-stats.json; webpack-bundle-analyzer --port 4200 compilation-stats.json"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "http-vue-loader": "^1.4.2",
    "lodash": "^4.17.20",
    "mobile-detect": "^1.4.4",
    "tippy.js": "^6.2.7",
    "vue": "^2.6.11",
    "vue-analytics": "^5.22.1",
    "vue-js-modal": "^2.0.0-rc.6",
    "vue-scrollmagic": "^1.2.0",
    "vue-youtube": "^1.4.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12"
  }
}
